{% extends 'base.html.twig' %}

{% block title %}New Time Slot{% endblock %}

{% block body %}
    <h1>Create new Time Slot</h1>

    {{ form_start(form) }}
        {{ form_row(form.start_at) }}
        {{ form_row(form.end_at) }}
        <button type="submit" class="btn btn-primary">Save</button>
    {{ form_end(form) }}

    <a href="{{ path('homepage') }}" class="btn btn-secondary">Back to list</a>
{% endblock %}